{
    "name": "opendtu-react-native",
    "version": "0.11.1",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenDTU-App/opendtu-react-native.git"
    },
    "bugs": {
        "url": "https://github.com/OpenDTU-App/opendtu-react-native/issues"
    },
    "license": "gpl-3.0",
    "scripts": {
        "android": "react-native run-android --active-arch-only",
        "android:release": "yarn android --mode release",
        "ios": "react-native run-ios",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "typecheck": "tsc --project tsconfig.json --pretty --noEmit --skipLibCheck",
        "start": "react-native start",
        "test": "echo 'No tests specified' && exit 0",
        "postinstall": "yarn run npm-license-crawler -onlyDirectDependencies -json licenses.json && patch-package && ./postinstall.sh",
        "postuninstall": "yarn run npm-license-crawler -onlyDirectDependencies -json licenses.json",
        "build-android": "bash -c \"pushd android ; ./gradlew assembleRelease ; popd\"",
        "postversion": "react-native-version --never-amend --skip-tag -L",
        "prepare": "node-git-hooks",
        "clean:android": "bash -c \"pushd android ; ./gradlew clean ; popd\"",
        "translations:push": "bash -c \"pushd src/translations/translation-files ; git add . ; git commit -m 'Update translations' ; git push ; popd\"",
        "translations:pull": "bash -c \"pushd src/translations/translation-files ; git pull ; popd\"",
        "precommit-hooks": "bash -c \".git/hooks/pre-commit\"",
        "fix-imports": "./fix-imports.mjs",
        "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install"
    },
    "dependencies": {
        "@octokit/rest": "^22.0.1",
        "@os-team/i18next-react-native-language-detector": "^1.1.6",
        "@pchmn/expo-material3-theme": "^1.4.0",
        "@react-native-async-storage/async-storage": "^2.0.0",
        "@react-native-clipboard/clipboard": "^1.16.0",
        "@react-native-community/netinfo": "^11.3.3",
        "@react-native-masked-view/masked-view": "^0.3.2",
        "@react-native/new-app-screen": "0.81.4",
        "@react-navigation/bottom-tabs": "^7.12.0",
        "@react-navigation/native": "^7.1.28",
        "@react-navigation/native-stack": "^7.12.0",
        "@reduxjs/toolkit": "^2.11.2",
        "@shopify/flash-list": "^2.2.1",
        "base-64": "^1.0.0",
        "compare-versions": "^6.1.1",
        "expo": "54.0.12",
        "fast-deep-equal": "^3.1.3",
        "fuse.js": "^7.1.0",
        "i18next": "^25.8.0",
        "intl-pluralrules": "^2.0.1",
        "ip-regex": "^5.0.0",
        "moment": "^2.30.1",
        "node-git-hooks": "1.0.7",
        "npm-license-crawler": "^0.2.1",
        "patch-package": "^8.0.1",
        "postinstall-postinstall": "^2.1.0",
        "pretty-bytes": "^7.1.0",
        "prometheus-query": "^3.5.1",
        "react": "19.1.0",
        "react-i18next": "^16.5.4",
        "react-native": "0.81.4",
        "react-native-charts-wrapper": "^0.6.0",
        "react-native-config": "1.5.6",
        "react-native-edge-to-edge": "^1.7.0",
        "react-native-fast-image": "^8.6.3",
        "react-native-flex-layout": "^0.1.5",
        "react-native-fs": "^2.20.0",
        "react-native-get-random-values": "^2.0.0",
        "react-native-linear-gradient": "^2.8.3",
        "react-native-localize": "^3.6.1",
        "react-native-logs": "^5.5.0",
        "react-native-markdown-display": "^7.0.2",
        "react-native-mmkv": "~3.3.3",
        "react-native-paper": "5.14.5",
        "react-native-paper-dates": "^0.23.3",
        "react-native-reanimated": "^4.2.1",
        "react-native-safe-area-context": "^5.6.2",
        "react-native-screens": "^4.21.0",
        "react-native-share": "^12.2.5",
        "react-native-simple-gauge": "^0.3.1",
        "react-native-skeleton-placeholder": "^5.2.4",
        "react-native-splash-screen": "^3.3.0",
        "react-native-svg": "^15.15.1",
        "react-native-toast-message": "^2.3.3",
        "react-native-url-polyfill": "^3.0.0",
        "react-native-vector-icons": "^10.3.0",
        "react-native-worklets": "^0.7.2",
        "react-native-zeroconf": "^0.14.0",
        "react-redux": "^9.2.0",
        "redux": "^5.0.1",
        "redux-persist": "^6.0.0",
        "styled-components": "^6.3.8",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.28.5",
        "@babel/plugin-transform-class-static-block": "^7.28.3",
        "@babel/plugin-transform-private-methods": "^7.27.1",
