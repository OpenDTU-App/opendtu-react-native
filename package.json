{
  "name": "opendtu-react-native",
  "version": "0.4.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenDTU-App/opendtu-react-native.git"
  },
  "bugs": {
    "url": "https://github.com/OpenDTU-App/opendtu-react-native/issues"
  },
  "license": "gpl-3.0",
  "scripts": {
    "android": "react-native run-android",
    "android:release": "yarn android --mode release",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --project tsconfig.json --pretty --noEmit --skipLibCheck",
    "start": "react-native start",
    "test": "jest --passWithNoTests",
    "postinstall": "yarn run npm-license-crawler -onlyDirectDependencies -json licenses.json && patch-package",
    "postuninstall": "yarn run npm-license-crawler -onlyDirectDependencies -json licenses.json",
    "build-android": "bash -c \"pushd android ; ./gradlew assembleRelease ; popd\"",
    "postversion": "react-native-version --never-amend --skip-tag",
    "prepare": "node-git-hooks",
    "clean:android": "bash -c \"pushd android ; ./gradlew clean ; popd\"",
    "translations:push": "bash -c \"pushd src/translations/translation-files ; git add . ; git commit -m 'Update translations' ; git push ; popd\"",
    "translations:pull": "bash -c \"pushd src/translations/translation-files ; git pull ; popd\"",
    "precommit-hooks": "bash -c \".git/hooks/pre-commit\"",
    "fix-imports": "./fix-imports.mjs"
  },
  "dependencies": {
    "@os-team/i18next-react-native-language-detector": "^1.0.34",
    "@pchmn/expo-material3-theme": "^1.3.2",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-clipboard/clipboard": "^1.14.1",
    "@react-native-community/netinfo": "^11.3.2",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "@react-navigation/bottom-tabs": "^6.6.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.10.0",
    "@reduxjs/toolkit": "^2.2.6",
    "base-64": "^1.0.0",
    "compare-versions": "^6.1.0",
    "expo": "^51.0.17",
    "i18next": "^23.11.5",
    "intl-pluralrules": "^2.0.1",
    "ip-regex": "^5.0.0",
    "moment": "^2.30.1",
    "node-git-hooks": "^1.0.7",
    "npm-license-crawler": "^0.2.1",
    "octokit": "^4.0.2",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "pretty-bytes": "^6.1.1",
    "prometheus-query": "^3.4.0",
    "react": "18.3.1",
    "react-i18next": "^14.1.2",
    "react-native": "0.74.3",
    "react-native-charts-wrapper": "^0.6.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-flex-layout": "^0.1.5",
    "react-native-fs": "^2.20.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-logs": "^5.1.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-paper": "^5.12.3",
    "react-native-paper-dates": "^0.22.12",
    "react-native-reanimated": "^3.12.1",
    "react-native-safe-area-context": "^4.10.7",
    "react-native-screens": "^3.32.0",
    "react-native-simple-gauge": "^0.3.1",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.3.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.1.0",
    "react-native-zeroconf": "^0.13.8",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.11",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-transform-class-static-block": "^7.24.4",
    "@babel/plugin-transform-private-methods": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/runtime": "^7.24.5",
    "@react-native/babel-preset": "0.74.85",
    "@react-native/eslint-config": "0.74.85",
    "@react-native/metro-config": "0.74.84",
    "@react-native/typescript-config": "0.74.84",
    "@testing-library/react-native": "^12.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/markdown-it": "^14.1.1",
    "@types/npm-license-crawler": "^0.2.3",
    "@types/react": "^18.3.2",
    "@types/react-native-charts-wrapper": "^0.5.11",
    "@types/react-native-zeroconf": "^0.12.3",
    "@types/react-test-renderer": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-no-relative-import-paths": "^1.5.4",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.3.1",
    "ts-jest": "^29.1.5",
    "typescript": "~5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4",
  "resolutions": {
    "react-native-paper@^5.12.3": "patch:react-native-paper@npm%3A5.12.3#./.yarn/patches/react-native-paper-npm-5.12.3-1a888b7688.patch",
    "node-git-hooks@^1.0.7": "patch:node-git-hooks@npm%3A1.0.7#./.yarn/patches/node-git-hooks-npm-1.0.7-618e27eb9b.patch"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-keep-awake",
        "expo-font",
        "expo-asset",
        "expo-file-system",
        "expo-application"
      ]
    }
  }
}
