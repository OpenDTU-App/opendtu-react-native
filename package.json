{
    "name": "opendtu-react-native",
    "version": "0.6.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenDTU-App/opendtu-react-native.git"
    },
    "bugs": {
        "url": "https://github.com/OpenDTU-App/opendtu-react-native/issues"
    },
    "license": "gpl-3.0",
    "scripts": {
        "android": "react-native run-android",
        "android:release": "yarn android --mode release",
        "ios": "react-native run-ios",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "typecheck": "tsc --project tsconfig.json --pretty --noEmit --skipLibCheck",
        "start": "react-native start",
        "test": "jest --passWithNoTests",
        "postinstall": "yarn run npm-license-crawler -onlyDirectDependencies -json licenses.json && patch-package && ./postinstall.sh",
        "postuninstall": "yarn run npm-license-crawler -onlyDirectDependencies -json licenses.json",
        "build-android": "bash -c \"pushd android ; ./gradlew assembleRelease ; popd\"",
        "postversion": "react-native-version --never-amend --skip-tag -L",
        "prepare": "node-git-hooks",
        "clean:android": "bash -c \"pushd android ; ./gradlew clean ; popd\"",
        "translations:push": "bash -c \"pushd src/translations/translation-files ; git add . ; git commit -m 'Update translations' ; git push ; popd\"",
        "translations:pull": "bash -c \"pushd src/translations/translation-files ; git pull ; popd\"",
        "precommit-hooks": "bash -c \".git/hooks/pre-commit\"",
        "fix-imports": "./fix-imports.mjs",
        "pod-install": "cd ios && pod install"
    },
    "dependencies": {
        "@octokit/rest": "^21.1.1",
        "@os-team/i18next-react-native-language-detector": "^1.1.6",
        "@pchmn/expo-material3-theme": "^1.3.2",
        "@react-native-async-storage/async-storage": "^2.1.2",
        "@react-native-clipboard/clipboard": "^1.16.2",
        "@react-native-community/netinfo": "^11.4.1",
        "@react-native-masked-view/masked-view": "^0.3.2",
        "@react-navigation/bottom-tabs": "^7.2.1",
        "@react-navigation/native": "^7.0.15",
        "@react-navigation/native-stack": "^7.2.1",
        "@reduxjs/toolkit": "^2.6.1",
        "@shopify/flash-list": "^1.7.3",
        "base-64": "^1.0.0",
        "compare-versions": "^6.1.1",
        "expo": "52.0.37",
        "fast-deep-equal": "^3.1.3",
        "fuse.js": "^7.1.0",
        "i18next": "^24.2.2",
        "intl-pluralrules": "^2.0.1",
        "ip-regex": "^5.0.0",
        "moment": "^2.30.1",
        "node-git-hooks": "1.0.7",
        "npm-license-crawler": "^0.2.1",
        "patch-package": "^8.0.0",
        "postinstall-postinstall": "^2.1.0",
        "pretty-bytes": "^6.1.1",
        "prometheus-query": "^3.4.1",
        "react": "18.3.1",
        "react-i18next": "^15.4.1",
        "react-native": "0.76.5",
        "react-native-charts-wrapper": "^0.6.0",
        "react-native-config": "^1.5.5",
        "react-native-fast-image": "^8.6.3",
        "react-native-flex-layout": "^0.1.5",
        "react-native-fs": "^2.20.0",
        "react-native-get-random-values": "^1.11.0",
        "react-native-linear-gradient": "^2.8.3",
        "react-native-localize": "^3.4.1",
        "react-native-logs": "^5.3.0",
        "react-native-markdown-display": "^7.0.2",
        "react-native-mmkv": "~2.12.2",
        "react-native-paper": "5.13.1",
        "react-native-paper-dates": "^0.22.34",
        "react-native-reanimated": "^3.17.1",
        "react-native-safe-area-context": "5.3.0",
        "react-native-screens": "^4.9.1",
        "react-native-share": "^12.0.9",
        "react-native-simple-gauge": "^0.3.1",
        "react-native-skeleton-placeholder": "^5.2.4",
        "react-native-splash-screen": "^3.3.0",
        "react-native-svg": "^15.11.2",
        "react-native-toast-message": "^2.2.1",
        "react-native-url-polyfill": "^2.0.0",
        "react-native-vector-icons": "^10.2.0",
        "react-native-zeroconf": "^0.13.8",
        "react-redux": "^9.2.0",
        "redux": "^5.0.1",
        "redux-persist": "^6.0.0",
        "styled-components": "^6.1.15",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.26.9",
        "@babel/plugin-transform-class-static-block": "^7.26.0",
        "@babel/plugin-transform-private-methods": "^7.25.9",
        "@babel/preset-env": "^7.26.9",
