                "expo-font",
                "expo-asset",
                "expo-file-system",
                "expo-application"
            ]
        }
    },
    "resolutions": {
        "@react-native/gradle-plugin": "0.76.5",
        "use-latest-callback": "0.2.3"
    }
}
